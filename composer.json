{
    "name": "swiftmade/blogdown",
    "description": "Simple Laravel blog package using markdown",
    "authors": [
        {
            "name": "Ahmet Ozisik",
            "email": "ozisikahmet@gmail.com"
        }
    ],
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "8.*|9.*",
        "graham-campbell/markdown": "14.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "laravel/laravel": "8.*",
        "mockery/mockery": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Swiftmade\\Blogdown\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/IntegrationTestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Swiftmade\\Blogdown\\BlogdownProvider"
            ]
        }
    }
}
